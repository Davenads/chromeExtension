{
  "manifest_version": 2,
  "name": "Healthy Reminders",
  "version": "1",
  "icons": {
    "128": "images/mountain.png"
  },
  "permissions": ["alarms"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Healthy Reminders",
    "default_popup": "index.html"
  },

  "minimum_chrome_version": "28"
}
